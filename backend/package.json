{
  "name": "hospital-journal-backend",
  "version": "1.0.0",
  "description": "协和医院SCI期刊分析系统后端API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration --runInBand --coverage",
    "migrate": "sequelize-cli db:migrate",
    "migrate:test": "NODE_ENV=test sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "db:reset:test": "NODE_ENV=test sequelize-cli db:drop && NODE_ENV=test sequelize-cli db:create && NODE_ENV=test sequelize-cli db:migrate",
    "security-test": "node ../scripts/security-test.js",
    "performance-test": "node ../scripts/performance-test.js",
    "test:all": "npm run test && npm run test:integration && npm run security-test && npm run performance-test"
  },
  "keywords": [
    "hospital",
    "journal",
    "statistics",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "pdfkit": "^0.13.0",
    "redis": "^4.6.0",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3"
  }
}
